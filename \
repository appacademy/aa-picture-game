class GameItem {
  constructor(data) {
    this.data = data;
    this.data.accuracy = 0;
  }

  compareTo(other) {
    if (this.data.accuracy < other.data.accuracy) {
      return 1;
    } else if(this.data.accuracy < other.data.accuracy) {
      return -1;
    } else {
      return 0;
    }
  }
}

module.exports = GameItem;
